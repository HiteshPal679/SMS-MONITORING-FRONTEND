{
  "expo": {
    "name": "SMS-SYNC-APP",
    "slug": "sms-sync-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.hiteshpal.smssyncapp",
      "permissions": [
        "READ_SMS",
        "RECEIVE_SMS"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "ios": {
      "bundleIdentifier": "com.hiteshpal.smssyncapp"
    }
  }
}
